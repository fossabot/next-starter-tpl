{
  "name": "@wayofdev/ui",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ],
    "./tailwind": {
      "require": "./src/tailwind.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist .turbo",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\" --fix"
  },
  "dependencies": {
    "@swc/helpers": "^0.4.14",
    "clsx": "^1.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@wayofdev/eslint-config-custom": "workspace:*",
    "eslint": "^8.30.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "next": "*",
    "react": "^18.2",
    "react-dom": "^18.2"
  }
}
